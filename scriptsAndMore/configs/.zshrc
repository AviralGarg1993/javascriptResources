# CUSTOM  
alias zshconfig="mate ~/.zshrc"
alias ohmyzsh="mate ~/.oh-my-zsh"
# alias lct='colorls --tree'
# alias lc='colorls -A --sd'
# alias ls='colorls --dark --sort-dirs --report'
alias ls='colorls -A --sort-dirs --tree'
