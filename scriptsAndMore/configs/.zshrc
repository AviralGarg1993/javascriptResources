# add node, npm (usr/local/bin) and maven into $PATH
export PATH="/usr/local/bin:/opt/apache-maven-3.6.3/bin:$PATH"
export JAVA_HOME="/Library/Java/JavaVirtualMachines/jdk-14.0.1.jdk/Contents/Home"
